					<entry key="date_my" value="china,brazil_import,honduras_import,honduras_export,korea,mexicon_import,uk_import,vnrl_import,vnrl_export"></entry>
					<!-- 配置日期的格式显示yyyy-mm-dd国家:除上面之前的所有的国家都是按照下面的这一个格式显示日期的 -->
					<entry key="date_ymd" value="other_all"></entry>
					<!-- 配置是否使用线程的标识: nouse_thread:代表不开启线程功能 ,use_thread代表使用线程功能-->
					<!-- <entry key="thread_falg" value="nouse_thread"></entry> -->
					<entry key="type_thread_falg" value="nouse_thread"></entry>
					<entry key="record_thread_falg" value="nouse_thread"></entry>
					<!-- 多线程每次读取的记录条数 -->
					<entry key="thread_pagesize" value="30000"></entry>
					<!-- 配置汇总默认显示的记录条数 -->
					<entry key="default_size" value="10"></entry>
					<!--新增流失报告参数标志-->
					<entry key="addoff" value="jkscompare_first,jkscompare_second,ckscompare_first,ckscompare_second,companycompare_first,companycompare_second"></entry>
					<!--需要处理标点符号的字符串 -->
					<entry key="handleStr" value="customs,dest_country,importer,exporter,start_port,end_port,notifier,manufacture,origin_country"></entry>
					<!-- 是否启用集合的过滤功能 no不启用 yes启用 -->
					<entry key="useFilter" value="no"></entry>
			</map>
		</constructor-arg>
	</bean>
